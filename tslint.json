{
    "extends": "tslint-config-standard",
    "globals": {
      "require": true
    },
    "rules": {
      "ter-indent": [
        true,
        4,
        {
            "SwitchCase": 1
        }
      ],
      "no-unused-expression": false,
      "one-line": false,
      "whitespace": false,
      "space-before-function-paren": false,
      "semicolon": false,
      "trailing-comma": false,
      "no-trailing-whitespace": false,
      "comment-format": false,
      "quotemark": false
    }
  }